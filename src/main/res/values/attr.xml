<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ImagePagerView">
        <attr name="images" format="reference"/>
        <attr name="transformer" >
            <enum name="none" value="0" />
            <enum name="zoomout" value="1" />

        </attr>

    </declare-styleable>

    <declare-styleable name="DotIndicator">
        <attr name="slider" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AppTabLayout">
        <attr name="background" format="reference"/>
        <attr name="stripPos">
            <enum name="top" value="1" />
            <enum name="bottom" value="2" />
        </attr>
    </declare-styleable>

</resources>